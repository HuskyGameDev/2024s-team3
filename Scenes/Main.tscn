script/source = "extends Node2D


func _on_cauldron_potion_made(potion):
	add_child(potion)
	## \"throws\" potion
	## angle options: +/- 250, 350, 450, and 550
	var throwAngle = (randi_range(2, 5) * 100 + 50)
	if(bool(randi_range(0, 1))):
		throwAngle *= -1
	potion.rigidbody.apply_central_impulse(Vector2(throwAngle, -2000))


func _on_shelf_body_entered(body):
	body.rotation = 0
	print(\"entered\")
